{
  "name": "gitlab-time-tracker",
  "version": "1.7.39",
  "description": "A command line interface for GitLabs time tracking feature.",
  "bugs": {
    "url": "https://github.com/kriskbx/gitlab-time-tracker/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kriskbx/gitlab-time-tracker.git"
  },
  "main": "src/gtt.js",
  "scripts": {
    "test": "NODE_ENV=test mocha 'spec/**/*.spec.js'",
    "cover": "istanbul cover _mocha $(find ./spec -name \"*.spec.js\")",
    "coveralls": "yarn run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "build": "pkg src/gtt.js -o out/gtt -c package.json"
  },
  "bin": {
    "gtt": "src/gtt.js"
  },
  "engines": {
    "node": ">=8.17"
  },
  "pkg": {
    "scripts": "src/**/*.js",
    "targets": [
      "node10-linux-x64",
      "node10-macos-x64",
      "node10-win-x64"
    ]
  },
  "author": "kriskbx",
  "license": "GPL-2.0",
  "dependencies": {
    "app-module-path": "^2.2.0",
    "async": "^3.2.0",
    "cli-cursor": "^3.1.0",
    "cli-table": "^0.3.1",
    "colors": "^1.4.0",
    "commander": "kriskbx/commander.js",
    "csv-string": "^2.3.2",
    "find-in-files": "^0.5.0",
    "hash-sum": "^2.0.0",
    "hashids": "^2.2.1",
    "markdown-pdf": "^10.0.0",
    "markdown-table": "^2.0.0",
    "moment": "^2.26.0",
    "moment-timezone": "^0.5.31",
    "node-spinner": "^0.0.4",
    "open": "^0.0.5",
    "progress": "^2.0.3",
    "prompt": "^1.0.0",
    "read-yaml": "^1.1.0",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.4",
    "shelljs": "^0.8.4",
    "throttled-queue": "^1.0.7",
    "underscore": "^1.10.2",
    "xdg-basedir": "^4.0.0",
    "xlsx": "^0.16.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^7.2.0",
    "pkg": "^4.4.8",
    "sinon": "^9.0.2"
  }
}
